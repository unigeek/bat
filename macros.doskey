alias   = doskey $*
cat     = type $*
clear   = cls
cp      = copy $*
cpr     = xcopy $*
grep    = find $*
history = doskey /history
kill    = taskkill /PID $*
ls      = dir $*
man     = help $*
mv      = move $*
ps      = tasklist $*
pwd     = echo %cd%
cd      = pushd $*
rm      = del $*
rmr     = deltree $*
sudo    = runas /user:administrator $*
home    = pushd %USERPROFILE%
